/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/interrupt-controller/irq.h>

#include "qca953x.dtsi"

/ {
	model = "MikroTik SXTsq Lite60";
	compatible = "mikrotik,sxtsq_lite60", "qca,qca9531";

	chosen {
		bootargs = "console=ttyS0,115200n8 wil6210.dyndbg=+p";
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&swphy4>;
//	mtd-mac-address = <&art 0x0>;
};

&eth1 {
//	mtd-mac-address = <&art 0x6>;
};

&pcie0 {
	status = "okay";
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "flash";
				reg = <0x000000 0x1000000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
